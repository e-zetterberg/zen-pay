{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:@next/next/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "camelcase":[2, {"ignoreImports": true}],
        "react/prop-types": [0],
        "react/function-component-definition": [2, {
            "namedComponents": "arrow-function",
            "unnamedComponents": "arrow-function"
          }]

    }
}
